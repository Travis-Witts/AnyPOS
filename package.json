  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "concurrently": "^6.1.0",
    "connect-session-sequelize": "^7.1.1",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.0",
    "sequelize": "^6.6.2"
  }
}
